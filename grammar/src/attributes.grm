/**
## Annotations
*/

annotations
  : (annotation | annotationList)*
  ;

annotation
  : annotationPrefix? unescapedAnnotation
  ;

annotationList
  : annotationPrefix "[" unescapedAnnotation+ "]"
  ;

annotationPrefix:
  : ("@" (":" ("file" | "field"))?)
  ;

unescapedAnnotation
  : SimpleName{"."} typeArguments? valueArguments?
  ;